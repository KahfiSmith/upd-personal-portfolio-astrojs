---
const { title = 'Coming Soon', note = 'Sedang disiapkan' } = Astro.props as { title?: string; note?: string };
---
<section style="display:grid; place-items:center; min-height:40vh; text-align:center">
  <div>
    <h2 style="margin:0 0 8px">{title}</h2>
    <p style="color:#666; margin:0">{note}</p>
  </div>
  
</section>

